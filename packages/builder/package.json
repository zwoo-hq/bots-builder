{
  "name": "@zwoo/bots-builder",
  "version": "0.1.0",
  "description": "A framework for building zwoo bots.",
  "author": "Fabian Kachlock",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zwoo-hq/bots-builder.git"
  },
  "bugs": {
    "url": "https://github.com/zwoo-hq/bots-bots-builder/issues"
  },
  "homepage": "https://github.com/zwoo-hq/bots-bots-builder/blob/main/README.md",
  "keywords": [
    "zwoo",
    "zwoo-bot"
  ],
  "files": [
    "dist",
    "src/context.d.ts",
    "cli.js"
  ],
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./globals": {
      "import": "./dist/globals.mjs",
      "types": "./dist/globals.d.ts"
    },
    "./context.d.ts": "./src/context.d.ts"
  },
  "type": "module",
  "bin": {
    "zwoo-bots-builder": "cli.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.12.5",
    "minimist": "^1.2.8",
    "picocolors": "^1.1.1",
    "typescript": "^5.4.4",
    "unbuild": "^2.0.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1"
  },
  "packageManager": "pnpm@9.12.0+sha256.a61b67ff6cc97af864564f4442556c22a04f2e5a7714fbee76a1011361d9b726",
  "dependencies": {
    "esbuild": "^0.24.0"
  }
}
